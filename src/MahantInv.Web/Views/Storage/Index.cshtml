@{
    ViewData["Title"] = "Storage";
}

<div class="card bg-dark border-dark">
    <div class="card-header">
        <span class="font-weight-bold text-white">Storage</span>
        <span class="float-right">
            <button class="btn btn-outline-primary btn-sm" type="button" onclick="Common.OpenModal(this)" data-id="0" data-target="AddEditStorage">
                <span>New</span>
            </button>
        </span>
    </div>
    <div class="card-body ag-theme-balham-auto-dark" id="storagedata">
    </div>
</div>
@await Html.PartialAsync("_AddEditStorage",new MahantInv.Infrastructure.Entities.Storage())
@section Scripts{
<script src="~/Storage/index.js" type="text/javascript" asp-append-version="true"></script>
}
